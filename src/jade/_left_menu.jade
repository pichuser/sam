div.left-menu
    input(type="checkbox",id="chk_open_menu", style="display:none")
    label.left-menu__menu-icon(for='chk_open_menu')
        include ../img/toBase64/menu_icon.svg
    nav.left-menu-menu
        each val in left_menu.items
            div.left-menu-menu__item(class= val.class)= val.name
        div.left-menu-menu__get-webinar
            a.link_dashed Заказать вебинар
    input(type="checkbox",id="open_contacts", style="display:none")
    div.left-menu__contact-info
        include _contact_info
    div.soc-icon
        div.soc-icon__item
            include ../img/toBase64/facebook_icon.svg
            //div.facebook_icon
        div.soc-icon__item
            include ../img/toBase64/vk_icon.svg
            //div.vk_icon
        div.soc-icon__item
            include ../img/toBase64/gplus_icon.svg
            //div.gplus_icon


